# xterm.trm for X11

# Screen commands

auto-margin	= 1
move-cursor-ti	= "\e[%i%p1%d;%p2%dH"
move-cursor-tc	= "\e[%i%d;%dH"
clear-screen	= "\e[2J"
clear-to-eol	= "\e[K"
clear-to-eos	= "\e[J"
add-line	= "\e[L"
delete-line	= "\e[M"
insert-char	= "\e[@"
delete-char	= "\e[P"

cursor-on	= "\e[?25h"
cursor-off	= "\e[?25l"

mouse-on	= "\e[?1000h"
mouse-off	= "\e[?1000l"
mouse-input	= "\e[M%i%+ %+ %+ "

alt-chars-ena	= "\e(B\e)0"
session-end	= "\ec"

# Key definitions

f1		= "\e[11~"
f1		= "\eOP"
