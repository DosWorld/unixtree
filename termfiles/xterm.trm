delete		= "\e[3~"

lines		= 24
cols		= 80

# Screen Attributes

bold-on		= "\e[1m"
bold-off	= "\e[m"
underline-on	= "\e[4m"
underline-off	= "\e[m"
blink-on	= "\e[5m"
blink-off	= "\e[m"
reverse-on	= "\e[7m"
reverse-off	= "\e[m"

alt-char-1-on	= "^N"
alt-char-1-off	= "^O"

